<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" styleClass="modal-bg" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="winnerLabel" alignment="CENTER" layoutX="25.0" layoutY="30.0" prefHeight="100.0" prefWidth="250.0" text="Winner!" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="closeButton" layoutX="100.0" layoutY="140.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Close">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
